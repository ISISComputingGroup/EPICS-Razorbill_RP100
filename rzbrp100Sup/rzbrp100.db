record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL, "$(RECSIM=0)")
    field(PINI, "YES")
}

record(bo, "$(P)DISABLE")
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(stringin, "$(P)ID")
{
    field(PINI, "YES")
	field(DESC, "ID string for device")
    field(DTYP, "stream")
    field(INP, "@rzbrp100.proto getID $(PORT)")
    field(SDIS, "$(P)DISABLE")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:ID")
    
    info(INTEREST, "MEDIUM")
	info(archive, "VAL")
}

record(stringout, "$(P)SIM:ID")
{
}

record(ai, "$(P)CH1:VOLTAGE")
{
	field(DESC, "Channel 1 Voltage")
	field(DTYP, "stream")
    field(INP, "@rzbrp100.proto getCH1Voltage $(PORT)")
	field(SCAN, "1 second")
	field(EGU, "V")
	field(ASG, "READONLY")
	field(SDIS, "$(P)DISABLE")
	field(PREC, "3")
	
	info(INTEREST, "HIGH")
	info(archive, "VAL")
	info(alarm, "RZBRP100")
}

record(ai, "$(P)CH2:VOLTAGE")
{
	field(DESC, "Channel 2 Voltage")
	field(DTYP, "stream")
    field(INP, "@rzbrp100.proto getCH2Voltage $(PORT)")
	field(SCAN, "1 second")
	field(EGU, "V")
	field(ASG, "READONLY")
	field(SDIS, "$(P)DISABLE")
	field(PREC, "3")
	
	info(INTEREST, "HIGH")
	info(archive, "VAL")
}

record(ao, "$(P)CH1:VOLTAGE:SP") 
{
	field(DESC, "Channel 1 Voltage setpoint")
    field(DTYP, "stream")
    field(OUT,  "@rzbrp100.proto setCH1Voltage $(PORT)")
	field(PREC, "3")
	field(EGU, "V")

	info(INTEREST, "HIGH")
	info(archive, "VAL")

	field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:UNITS:SP")
    field(SDIS, "$(P)DISABLE")
}

record(ao, "$(P)CH2:VOLTAGE:SP") 
{
	field(DESC, "Channel 2 Voltage setpoint")
    field(DTYP, "stream")
    field(OUT,  "@rzbrp100.proto setCH2Voltage $(PORT)")
	field(PREC, "3")
	field(EGU, "V")

	info(INTEREST, "HIGH")
	info(archive, "VAL")

	field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:UNITS:SP")
    field(SDIS, "$(P)DISABLE")
